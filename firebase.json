{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/tournament/*/edit$",
        "destination": "/tournament/[id]/edit.html"
      },
      {
        "source": "/tournament/*/*",
        "destination": "/tournament/[id]/[...slug].html"
      },
      {
        "source": "/tournament/1",
        "destination": "/tournament/[id]/[...slug].html"
      },
      {
        "source": "/p2e/dashboard/history/*",
        "destination": "/p2e/dashboard/history/[id].html"
      },
      {
        "source": "/p2e/raffles/*",
        "destination": "/p2e/raffles/[id].html"
      },
      {
        "source": "/p2e/dashboard",
        "destination": "/p2e/dashboard.html"
      },
      {
        "source": "/p2e/missions",
        "destination": "/p2e/missions.html"
      },
      {
        "source": "/p2e/raffles",
        "destination": "/p2e/raffles.html"
      }
    ]
  },
  "emulators": {
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true,
      "port": 9000
    }
  }
}