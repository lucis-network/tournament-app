{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/arena",
        "destination": "/arena.html"
      },
      {
        "source": "/arena/*/edit$",
        "destination": "/arena/[id]/edit.html"
      },
      {
        "source": "/arena/*/*",
        "destination": "/arena/[id]/[...slug].html"
      },
      {
        "source": "/arena/*/*/**",
        "destination": "/arena/[id]/[...slug].html"
      },
      {
        "source": "/arena/1",
        "destination": "/arena/[id]/[...slug].html"
      },
      {
        "source": "/playcore/dashboard/history/*",
        "destination": "/playcore/dashboard/history/[id].html"
      },
      {
        "source": "/playcore/raffles/**",
        "destination": "/playcore/raffles/[id].html"
      },
      {
        "source": "/playcore/dashboard",
        "destination": "/playcore/dashboard.html"
      },
      {
        "source": "/playcore/missions",
        "destination": "/playcore/missions.html"
      },
      {
        "source": "/playcore/raffles",
        "destination": "/playcore/raffles.html"
      },
      {
        "source": "/playcore/lucky-chest",
        "destination": "/playcore/lucky-chest.html"
      },
      {
        "source": "/profile",
        "destination": "/profile.html"
      },
      {
        "source": "/profile/*",
        "destination": "/profile/[username].html"
      },
      {
        "source": "/profile/*",
        "destination": "/profile/[username].html"
      }
    ]
  },
  "emulators": {
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true,
      "port": 9000
    }
  }
}